<?xml version="1.0"?>
<openerp>
<data>

	<template id="vemo_report_invoice" inherit_id="account.report_invoice">
		<xpath expr="//t[@t-call='report.html_container']" position="replace">
		    <t t-call="report.html_container">
		        <t t-foreach="doc_ids" t-as="doc_id">
		            <t t-raw="translate_doc(doc_id, doc_model, 'partner_id.lang', 'aloxa_vemo.vemo_report_invoice_document')"/>
		        </t>
		    </t>
	    </xpath>
	</template>
	
</data>
</openerp>
